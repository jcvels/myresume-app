include resume-date-former.pug

h2.section-title Education
article.resume-main-article
  each education in data.en.education
    .left-title
      h3 #{education.institute.name}
    .right-title
      each course in education.courses
        div
          h4.item-title #{course.title}
          +date-former(course.start,course.end)
          p.item-description #{course.description}